<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.2.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>jQuery: Replaces all matched elements with the specified HTML or DOM elements.</Title>
      <Author>John Sheehan (@johnsheehan)</Author>
      <Shortcut>jqReplaceWith</Shortcut>
      <HelpUrl>https://github.com/xxkylexx/Visual-Studio-jQuery-Code-Snippets</HelpUrl>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>selector</ID>
          <ToolTip>Element(s) to be modified</ToolTip>
          <Default>selector</Default>
        </Literal>
        <Literal>
          <ID>content</ID>
          <ToolTip>Content to use as replacement</ToolTip>
          <Default>content</Default>
        </Literal>
      </Declarations>
      <Code Language="JavaScript">
        <![CDATA[$$($selector$).replaceWith($content$);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

