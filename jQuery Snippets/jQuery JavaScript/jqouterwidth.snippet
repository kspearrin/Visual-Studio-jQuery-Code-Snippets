<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.1.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>jQuery: Gets the outer width of the first matched element.</Title>
      <Author>John Sheehan (@johnsheehan)</Author>
      <Shortcut>jqouterwidth</Shortcut>
      <HelpUrl>http://jquerysnippets.codeplex.com</HelpUrl>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>selector</ID>
          <ToolTip>jQuery selector</ToolTip>
          <Default>selector</Default>
        </Literal>
        <Literal>
          <ID>includeMarginBool</ID>
          <ToolTip>Boolean value indicating whether or not to include margin</ToolTip>
          <Default>includeMarginBool</Default>
        </Literal>
      </Declarations>
      <Code Language="JavaScript">
        <![CDATA[$$($selector$).outerWidth($includeMarginBool$);]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

