<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.4.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>jQuery: die(functionName)</Title>
      <Author>John Sheehan (@johnsheehan)</Author>
      <Shortcut>jqDieFn</Shortcut>
      <HelpUrl>https://github.com/kspearrin/Visual-Studio-jQuery-Code-Snippets</HelpUrl>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>selector</ID>
          <ToolTip>Element(s) to unbind</ToolTip>
          <Default>selector</Default>
        </Literal>
        <Literal>
          <ID>functionName</ID>
          <ToolTip>Function to unbind</ToolTip>
          <Default>functionName</Default>
        </Literal>
      </Declarations>
      <Code Language="JavaScript">
        <![CDATA[$$($selector$).die($functionName$);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

